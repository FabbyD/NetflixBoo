{
	"manifest_version" : 2,

	"name" : "NetflixBoo",
	"description" : "Netflix extension to synchronize your netflix and chill sessions across the ocean",
	"version" : "1.0",

	"background" : {
		"scripts" : ["js/background.js"],
		"persistent" : false
	},

	"page_action" : {
		"default_icon" : "images/pacman.png"
	},

	"permissions" : [
		"tabs",
		"activeTab",
		"declarativeContent",
		"https://ajax.googleapis.com/"
	]
}
